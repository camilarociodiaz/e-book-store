{
  "expo": {
    "name": "ebook-store",
    "slug": "ebook-store",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/logo-app.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.camila.bookstore",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-app.png",
        "backgroundColor": "#fafafa"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "config": {
      "googleMaps": {
        "apiKey": "AIzaSyCyGeQ0XUgO6II5VMqUxQaBt3JjZQN480A"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ba513d59-a790-411e-9ece-34b560e2a7bb"
      }
    }
  }
}
